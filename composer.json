{
    "name": "brick/geo-doctrine",
    "description": "Doctrine types & functions for brick/geo",
    "type": "library",
    "keywords": [
        "Brick",
        "Geo",
        "Doctrine"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "brick/geo": "~0.8.0 || ~0.9.0",
        "doctrine/orm": "^2.8",
        "doctrine/dbal": "^3.3"
    },
    "require-dev": {
        "doctrine/annotations": "^1.0",
        "doctrine/cache": "^1.11",
        "doctrine/data-fixtures": "^1.0",
        "phpunit/phpunit": "^9.0",
        "php-coveralls/php-coveralls": "^2.0",
        "vimeo/psalm": "5.15.0"
    },
    "autoload": {
        "psr-4": {
            "Brick\\Geo\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brick\\Geo\\Doctrine\\Tests\\": "tests/"
        }
    }
}
